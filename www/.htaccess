# Enable RewriteEngine
RewriteEngine On
RewriteBase /

# Serve existing files/directories
RewriteCond %{REQUEST_FILENAME} -f
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# Route .php files
RewriteCond %{REQUEST_URI} !\.
RewriteRule ^(.+)$ $1.php [L]

# Root to index
RewriteRule ^$ index.php [L]
